{
    "name": "acelords/core",
    "description": "The core system for AceLords Products",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "gladcodes/keygen": "^1.1",
        "laravel/framework": "^5.6",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "AceLords\\Core\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AceLords\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AceLords\\Core\\Providers\\AceLordsCoreServiceProvider",
                "AceLords\\Core\\Providers\\RedisServiceProvider",
                "AceLords\\Core\\Providers\\ComposerServiceProvider"
            ]
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Lexx YungCarter",
            "email": "spencermainaa@gmail.com",
            "homepage": "https://github.com/lexxyungcarter",
            "role": "Founder"
        },
        {
            "name": "AceLords System Engineers",
            "email": "info@acelords.space",
            "homepage": "https://acelords.space",
            "role": "Collaborators"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
